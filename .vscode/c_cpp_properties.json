{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "${default}",
            "includePath": [
                "${workspaceFolder}/**",
                "C:/esp/v5.5.1/esp-idf/components/**"
            ],
            "defines": [
                "ESP_PLATFORM"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-gcc-x64",
            "configurationProvider": "espressif.esp-idf-extension"
        }
    ],
    "version": 4
}